{
	"name": "Gen2ParquetDataset",
	"properties": {
		"linkedServiceName": {
			"referenceName": "ADLSGen2",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"containerName": {
				"type": "string",
				"defaultValue": "landing"
			},
			"folderName": {
				"type": "string",
				"defaultValue": "nyc/taxi/yellow"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"folderPath": {
					"value": "@dataset().folderName",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().containerName",
					"type": "Expression"
				}
			},
			"compressionCodec": "none"
		},
		"schema": []
	}
}