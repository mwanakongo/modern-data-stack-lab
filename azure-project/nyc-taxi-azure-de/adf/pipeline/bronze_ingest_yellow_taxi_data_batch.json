{
	"name": "bronze_ingest_yellow_taxi_data_batch",
	"properties": {
		"description": "Copies the NYC yellow cab raw data from the landing to the the bronze raw data folder",
		"activities": [
			{
				"name": "bronze_ingest_yellow_taxi_data_batch",
				"type": "SynapseNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"snapshot": true
				}
			}
		],
		"variables": {
			"sourceFolderName": {
				"type": "String",
				"defaultValue": "nyc/taxi/yellow"
			},
			"sourceContainerName": {
				"type": "String",
				"defaultValue": "landing"
			}
		},
		"folder": {
			"name": "nyc"
		},
		"annotations": []
	}
}